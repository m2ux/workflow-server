version: 1.0.0
description: Default agent registry with specialized sub-agents for delegation

agents:
  rust-code-reviewer:
    effectivities[2]:
      - code-review_rust
      - code-review_rust_substrate
    model: fast

  general-code-reviewer:
    effectivities[1]:
      - code-review
    model: fast

  test-reviewer:
    effectivities[1]:
      - test-review
    model: fast

  pr-responder:
    effectivities[1]:
      - pr-review-response
    model: fast
